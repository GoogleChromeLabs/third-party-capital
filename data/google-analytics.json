{
  "id": "google-analytics",
  "description": "Install a Google Analytics tag on your website",
  "website": "https://analytics.google.com/analytics/web/",
  "scripts": [
    {
      "url": "https://www.googletagmanager.com/gtag/js",
      "params": ["id"],
      "optionalParams": {
        "l": null
      },
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "gtag"
    },
    {
      "code": "window[{{l}}]=window[{{l}}]||[];window['gtag-'+{{l}}]=function (){window[{{l}}].push(arguments);};window['gtag-'+{{l}}]('consent', 'default', {'ad_user_data': 'denied','ad_personalization': 'denied','ad_storage': 'denied','analytics_storage': 'denied','wait_for_update': 500,});window['gtag-'+{{l}}]('js',new Date());window['gtag-'+{{l}}]('config',{{id}})",
      "params": ["id"],
      "optionalParams": {
        "l": "dataLayer"
      },
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "setup"
    }
  ]
}
