{
    "id": "google-tag-manager",
    "description": "Install Google Tag Manager on your website",
    "website": "https://developers.google.com/tag-platform/tag-manager/web",
    "scripts": [
      {
        "url": "https://www.googletagmanager.com/gtm.js",
        "params": ["id"],
        "optionalParams": {
          "l": null
        },
        "strategy": "worker",
        "location": "head",
        "action": "append",
        "key": "gtm"
      },
      {
        "code": "window[{{l}}]=window[{{l}}]||[];(() => window[{{l}}].push(arguments))('consent', 'default', {ad_user_data: 'denied',ad_personalization: 'denied',ad_storage: 'denied',analytics_storage: 'denied',wait_for_update: 500});window[{{l}}].push({'gtm.start':new Date().getTime(),event:'gtm.js'});",
        "optionalParams": {
          "l": "dataLayer"
        },
        "strategy": "worker",
        "location": "head",
        "action": "append",
        "key": "setup"
      }
    ]
}