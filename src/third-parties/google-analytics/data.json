{
  "id": "google-analytics",
  "description": "Install a Google Analytics tag on your website",
  "website": "https://analytics.google.com/analytics/web/",
  "scripts": [
    {
      "url": "https://www.googletagmanager.com/gtag/js",
      "params": ["id"],
      "optionalParam": ["l"],
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "gtag"
    },
    {
      "code": "window['{{l}}'??'dataLayer']=window['{{l}}'??'dataLayer']||[];window['{{l}}'??'dataLayer'].push({'js':new Date()});window['{{l}}'??'dataLayer'].push({'config':'{{id}}'})",
      "params": ["id"],
      "optionalParam": ["l"],
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "setup"
    }
  ]
}
