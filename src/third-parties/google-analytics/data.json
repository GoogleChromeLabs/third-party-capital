{
  "id": "google-analytics",
  "description": "Install a Google Analytics tag on your website",
  "website": "https://analytics.google.com/analytics/web/",
  "scripts": [
    {
      "url": "https://www.googletagmanager.com/gtag/js",
      "params": ["id"],
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "gtag"
    },
    {
      "code": "window.dataLayers=window.dataLayers||{};window.dataLayers['{{dataLayerName}}']=window.dataLayers['{{dataLayerName}}']||[];window.dataLayers['{{dataLayerName}}'].push({'js':new Date()});window.dataLayers['{{dataLayerName}}'].push({'config':'{{id}}'})",
      "params": ["id", "dataLayerName"],
      "strategy": "worker",
      "location": "head",
      "action": "append",
      "key": "setup"
    }
  ]
}
